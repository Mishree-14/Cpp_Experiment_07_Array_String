#include <iostream>
using namespace std;

int main() {
    int arr[5]={52,81,50,37,89};
    //int arr2[5] = {16,84,27,94,60};
    int sum=0;
    
    for(int i=0;i<5;i++){
        cout<<arr[i]<<"\t";
    }
    cout<<"\n"<<"Sum of Array Elements: ";
    
    for(int i=0;i<5;i++){
        sum+=arr[i];
    }
    cout<<sum;
    cout<<"\n"<<"Average of Array Elements: ";
    cout<<sum/5;
    
    
    return 0;
}
